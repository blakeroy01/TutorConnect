
<h1> Tutors </h1>
<table>
    <thead>
        <tr>
            <th>Username</th>
            <th>Bio</th>
        </tr>
    </thead

    <tbody>
        <% @user.each do |user| %>
            <tr>
                <td><%= user.username %></td>
                <td><%= user.bio %></td>
                <td><%= link_to "Show", user_path(user) %>
            </tr>
        <% end %>
    </tbody>
</table>
            